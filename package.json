{
  "name": "JAMMS",
  "author": "Ian Santagata",
  "description": "JAMMS is Just a Music Maintainer for Spotify!  It is meant to modify, manipulate, and maintain Spotify playlists and libraries in ways that are currently not supported natively or easily within Spotify today.",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": "github:iansantagata/jamms",
  "scripts": {
    "lintjs": "eslint . --ignore-path .gitignore",
    "linthtml": "linthtml \"**/*.html\"",
    "lintvash": "linthtml \"**/*.vash\"",
    "lint": "npm run lintjs && npm run linthtml && npm run lintvash",
    "lintfix": "eslint . --fix --ignore-path .gitignore",
    "preinstall": "bash build/runGruntCommand.sh clean",
    "postinstall": "bash build/runGruntCommand.sh copy"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.5.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "express": "~4.17.3",
    "helmet": "^4.6.0",
    "jquery": "^3.5.1",
    "nodemailer": "^6.7.0",
    "querystring": "^0.2.1",
    "vash": "^0.13.0"
  },
  "devDependencies": {
    "@linthtml/linthtml": "^0.8.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.0",
    "grunt": "^1.5.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0"
  }
}
